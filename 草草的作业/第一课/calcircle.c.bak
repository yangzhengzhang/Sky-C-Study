#include <stdio.h>

#define PAI 3.14
#define RADIUS1 8
#define RADIUS2 8.18

main(void)
{

    printf("The perimeter of the circle with radius of 8 is : %.2f\n", 2 * RADIUS1 * PAI);
    printf("The perimeter of the circle with radius of 8.18 is : %.2f\n", 2 * RADIUS2 * PAI);

    return 0;

}
